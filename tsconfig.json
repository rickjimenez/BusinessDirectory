{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "esnext",
    "baseUrl": "./src",
    "paths": {
      "@customTypes/*": ["types/*"],
      "@styles/*": ["styles/*"],
      "@services/*": ["services/*"],
      "@components/*": ["components/*"],
      "@navigations/*": ["navigations/*"],
      "@screens/*": ["screens/*"],
      "@hooks/*": ["hooks/*"]
    }
  }
}
